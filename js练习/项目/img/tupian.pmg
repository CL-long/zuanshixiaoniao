    {
        var area = $('.ser_lnk1 > a');   // 设置目标区域

        if(getMyCookie(key) != 'true'){

            setTimeout(function (){
                setMyCookie(key, 'true', 24 * 60 * 60 * 1000);
                area.html(line1+"<br/>"+line2)
                area.addClass('on')
            }, time * 1000);

            setTimeout(function (){
                area.removeClass('on')
            }, time * 1000 + 8000);

            $(document).mouseup(function(e){
                area.removeClass('on')
            });
        }
    }

    // 写入cookie
    function setMyCookie(name, value, time)
    { 
        var exp = new Date(); 
        exp.setTime(exp.getTime() + time); 
        document.cookie = name + "=" + escape (value) + ";expires=" + exp.toGMTString(); 
    } 

    //读取 cookies 
    function getMyCookie(name) 
    { 
        var arr,reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");

        if(arr = document.cookie.match(reg)){
            return unescape(arr[2]); 
        }else {
            return null;
        }  
    } 
